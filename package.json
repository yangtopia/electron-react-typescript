{
  "name": "electron-react-ts",
  "version": "1.0.0",
  "description": "",
  "main": "./dist/main.js",
  "scripts": {
    "build:electron": "cross-env NODE_ENV=production webpack --config ./config/webpack.electron.prod.config.js",
    "build:renderer": "cross-env NODE_ENV=production webpack --config ./config/webpack.renderer.prod.config.js",
    "build": "npm run build:electron && npm run build:renderer",
    "lint": "eslint --ext=jsx,js,tsx,ts src",
    "dev:renderer": "webpack-dev-server --config ./config/webpack.renderer.dev.config.js",
    "dev:electron": "webpack --config ./config/webpack.electron.config.js && electron ./dist/main.js",
    "dev": "cross-env START_HOT=1 npm run dev:renderer",
    "pack": "npm run build && electron-builder --dir",
    "dist": "npm run build && electron-builder",
    "postinstall": "electron-builder install-app-deps"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "build": {
    "productName": "ProductName",
    "appId": "org.your.productname",
    "mac": {
      "category": "your.app.category.type"
    },
    "directories": {
      "output": "release"
    },
    "files": [
      "dist/",
      "node_modules/",
      "package.json"
    ],
    "linux": {
      "target": "deb"
    },
    "win": {
      "target": "nsis"
    }
